{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "web/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/web/index.html"
    }
  ],
  "env": {
    "MNEMONIC": "@mnemonic",
    "QUANTUM_KEY": "@quantum_key"
  }
}