{
  "name": "gulp-eslint-new",
  "version": "2.6.0",
  "description": "A gulp plugin to lint code with ESLint 8 and 9.",
  "keywords": [
    "gulpplugin",
    "eslint",
    "gulp",
    "errors",
    "warnings",
    "check",
    "source",
    "code",
    "formatter",
    "js",
    "javascript",
    "task",
    "lint",
    "plugin"
  ],
  "license": "MIT",
  "author": "Adametry",
  "files": [
    "lib"
  ],
  "main": "lib/gulp-eslint-new.js",
  "directories": {
    "example": "example",
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/origin-1/gulp-eslint-new.git"
  },
  "scripts": {
    "build": "npm install && gulp",
    "coverage": "gulp test",
    "lint": "eslint . --format compact",
    "release": "git add CHANGELOG.md && npm version -f",
    "test": "mocha --check-leaks test/*.spec.js",
    "ts-test": "tsc --project test/tsconfig.json"
  },
  "dependencies": {
    "eslint": "8 || 9 || 10",
    "fancy-log": "^2.0.0",
    "plugin-error": "^2.0.1",
    "semver": "^7.7.3",
    "ternary-stream": "^3.0.0",
    "vinyl-fs": "^4.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.1",
    "@origin-1/eslint-config": "^1.13.0",
    "@types/eslint__js": "^8.42.3",
    "c8js": "^0.8.1",
    "eslint-10.0": "npm:eslint@10.0.0",
    "eslint-8.0": "npm:eslint@8.0",
    "eslint-8.21": "npm:eslint@8.21",
    "eslint-8.x": "npm:eslint@8.x",
    "eslint-9.0": "npm:eslint@9.0",
    "eslint-9.x": "npm:eslint@9.x",
    "eslint-config-~shareable": "file:test/config",
    "eslint-formatter-~formatter": "file:test/formatter",
    "eslint-formatter-compact": "^9.0.1",
    "eslint-plugin-tsdoc": "^0.5.0",
    "globals": "^17.3.0",
    "gulp": "^5.0.1",
    "jiti": "^2.6.1",
    "mocha": "^9.2.2",
    "resolve": "^1.22.11",
    "typescript": "~5.9.3",
    "typescript_4.8": "npm:typescript@4.8",
    "typescript_5": "npm:typescript@5",
    "vinyl": "^3.0.1"
  },
  "optionalDependencies": {
    "@types/node": ">=12"
  },
  "engines": {
    "node": "^12.20 || ^14.13 || >=16"
  },
  "exports": {
    ".": "./lib/gulp-eslint-new.js",
    "./package.json": "./package.json"
  },
  "imports": {
    "#util": "./lib/util.js"
  },
  "type": "commonjs",
  "types": "lib/gulp-eslint-new.d.ts"
}
